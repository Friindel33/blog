<div class="page-header">
  <%= link_to photos_path, class: 'btn btn-default' do %>
  All photos
  <% end %>
  <%= link_to 'Back', photos_path %>
  <h1>Show Photo</h1>
</div>
<dl class="dl-horizontal">
  <dt>Title</dt>
  <dd><%= @photo.title %></dd>
<dt>Image:</dt>
  <dd><%= image_tag @photo.image_url if @photo.image %></dd>
</dl>
